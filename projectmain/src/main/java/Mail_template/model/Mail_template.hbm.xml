<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Mail_template.model.Mail_template" table="Mail_template" >
        
        <id name="mail_id" column="mail_id">
            <generator class="identity"/>
        </id>
        <property name="mail_name" column="mail_name" />
        <property name="mail_text" column="mail_text" />
    </class>
</hibernate-mapping>

