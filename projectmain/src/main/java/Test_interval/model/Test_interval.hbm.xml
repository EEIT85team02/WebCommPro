<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Test_interval.model.Test_intervalVO" table="Test_interval" >
        <composite-id>
     		<key-property name="class_id" column="class_id"></key-property>
        	<key-property name="test_startdate" column="test_startdate"></key-property>
        </composite-id>
        <property name="test_interval_id" column="test_interval_id"></property>
        <property name="test_enddate" column="test_enddate" />
        
    </class>
</hibernate-mapping>

