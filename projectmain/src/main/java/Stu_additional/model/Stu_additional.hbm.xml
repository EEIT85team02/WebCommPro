<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Stu_additional.model.Stu_additionalVO" table="Stu_additional" >
     
         <id name="stu_add_id" column="stu_add_id">
				<generator class="identity"></generator>
        </id>
        

        <property name="test_startdate" column="test_startdate" />
        <property name="test_hour_id" column="test_hour_id" />
        <property name="stu_applytime" column="stu_applytime" />
        <property name="emp_id" column="emp_id" />
        <property name="confirm_time" column="confirm_time" />


        
       	 <many-to-one name="StudentVO" class="Student.model.StudentVO" lazy="false">
            <column name="stu_id" />
        </many-to-one>  
        
         <set name="Mem_stas" table="Member_status" lazy="false" cascade="all"  inverse="true"  order-by="sta_id asc">			
			<key>
				<column name="sta_id" />
			</key>
			<one-to-many class="Member_status.model.Member_statusVO" />
		</set>

    </class>
</hibernate-mapping>
