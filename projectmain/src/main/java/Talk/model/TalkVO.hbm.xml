<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Talk.model.TalkVO" table="Talk" >
        
        <id name="talkId" column="talkId">
            <generator class="identity"/>
        </id>
        
        <property name="talkName" column="talkName" />
        <property name="talkGender" column="talkGender" />
        <property name="talkChose" column="talkChose" />
        <property name="talkContent" column="talkContent" />
    </class>
</hibernate-mapping>

